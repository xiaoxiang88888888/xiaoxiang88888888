<?xml version="1.0" encoding="GBK"?>
<config>
	<group name="deploy">
		<property name="deploy.apache.home" defaultValue="/usr/alibaba/httpd2" description="apache所在的目录" />
		<property name="deploy.java.home" defaultValue="/usr/alibaba/java" description="JDK所在的目录 " />
		<property name="deploy.jetty.home" defaultValue="/usr/alibaba/jetty" description="Jetty所在的目录 " />
			
		<property name="deploy.user" defaultValue="admin" description="静态资源所在的目录" />
		<property name="deploy.user.group" defaultValue="admin" description="静态资源所在的目录" />
			
		<property name="deploy.apache.port" defaultValue="80" description="apache开放的端口" />
		<property name="deploy.app.port" defaultValue="8080" description="jetty开放的端口" />
		<property name="deploy.debug.port" defaultValue="8000" description="jettyDebug开放的端口" />
		
		<property name="deploy.home" defaultValue="${user.home}/www" description="打包安装的目的文件夹" />
		<property name="deploy.output" defaultValue="${user.home}/www/output" description="Jetty，apache日志等输入目录" />
		
		<property name="deploy.web.domain" defaultValue="youhui.alibaba.com" description="动态资源所启用的域名" />
		<property name="deploy.taoapp.web.domain" defaultValue="taoapp.youhui.alibaba.com" description="在淘宝应用的域名" />
		<property name="deploy.web.home" defaultValue="${user.home}/www/static/html" description="静态资源所启在的目录" />
		<property name="deploy.static.domain" defaultValue="static.youhui.alibaba.com" description="静态资源所启用的域名" />
		<property name="deploy.static.home" defaultValue="${user.home}/www/static/assets" description="静态资源所在的目录" />
		
		<property name="deploy.production" defaultValue="false" description="是否是生产环境" />
		<property name="hummock.home" defaultValue="/usr/alibaba/hummock" description="hummock目录" />
	</group>
</config>